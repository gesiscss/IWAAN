#nbstripout --install
jupyter nbextension enable --py --sys-prefix qgrid
jupyter contrib nbextension install --user
jupyter nbextension enable --py widgetsnbextension
cp main.css /srv/conda/envs/notebook/lib/python3.8/site-packages/jupyter_contrib_nbextensions/nbextensions/exercise2
jupyter contrib nbextension install --user
jupyter nbextension enable init_cell/main
jupyter trust 1_General_Metadata.ipynb
jupyter trust 2_Page_analysis.ipynb
jupyter trust 3_Focus_on_editor.ipynb
jupyter nbextension enable hide_input_all/main
jupyter nbextension enable hide_input/main
jupyter nbextension enable exercise2/main
